{
  "name": "claude-ai-dev-assistant",
  "displayName": "Claude AI Development Assistant",
  "description": "AI-powered development with intelligent routing and agent management",
  "version": "0.1.0",
  "publisher": "ai-dev-infrastructure",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onStartupFinished"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "claudeAiDev.initializeProject",
        "title": "Initialize AI-Enhanced Project",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.importExistingProject",
        "title": "Import Existing Project",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.chat",
        "title": "AI Chat (Intelligent Routing)",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.codeReview",
        "title": "Review Selected Code",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.reviewProject",
        "title": "Review Entire Project",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.refactor",
        "title": "Refactor Selected Code",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.generateTests",
        "title": "Generate Tests",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.explainCode",
        "title": "Explain Selected Code",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.viewDashboard",
        "title": "View AI Dashboard",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.updateScope",
        "title": "Update Project Scope",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.generateCode",
        "title": "Generate Code at Cursor",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.acceptSuggestion",
        "title": "Accept Code Suggestion",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.rejectSuggestion",
        "title": "Reject Code Suggestion",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.loadChatHistory",
        "title": "Load Chat Session",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.saveChatSession",
        "title": "Save Chat Session",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.newChatSession",
        "title": "New Chat Session",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.exportChatSession",
        "title": "Export Chat to Markdown",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.reviewWithAnnotations",
        "title": "Review Code with Inline Annotations",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.clearReviewAnnotations",
        "title": "Clear Review Annotations",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.applyAllFixes",
        "title": "Apply All Review Fixes",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.browseAgents",
        "title": "Browse AI Agents",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.selectAgent",
        "title": "Select Agent for Next Task",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.submitFeedback",
        "title": "Submit Agent Feedback",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.clearAgentSelection",
        "title": "Clear Agent Selection (Use Auto)",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.generateDockerfile",
        "title": "Generate Dockerfile",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.generateCIPipeline",
        "title": "Generate CI/CD Pipeline",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.generateDockerCompose",
        "title": "Generate Docker Compose",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.generateFeature",
        "title": "Generate Multi-File Feature",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.scaffoldProject",
        "title": "Scaffold New Project",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.executeTool",
        "title": "Execute MCP Tool",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.chainTools",
        "title": "Build Tool Chain",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.selectTool",
        "title": "Select Tool for Task",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.showToolStatus",
        "title": "Show Tool Configuration Status",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.viewLearningInsights",
        "title": "View Agent Learning Insights",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.manageCustomRules",
        "title": "Manage Custom Review Rules",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.createCustomRule",
        "title": "Create Custom Rule from Selection",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.testCustomRule",
        "title": "Test Custom Rule on Selection",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.showPermissionStatus",
        "title": "Show Permission Status",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.configureRBAC",
        "title": "Manage Users (Admin)",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.viewComplianceReport",
        "title": "View Compliance Dashboard",
        "category": "Claude AI"
      },
      {
        "command": "claudeAiDev.exportAuditLog",
        "title": "Export Audit Log",
        "category": "Claude AI"
      }
    ],
    "keybindings": [
      {
        "command": "claudeAiDev.chat",
        "key": "ctrl+shift+a",
        "mac": "cmd+shift+a"
      },
      {
        "command": "claudeAiDev.codeReview",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r"
      },
      {
        "command": "claudeAiDev.generateTests",
        "key": "ctrl+shift+t",
        "mac": "cmd+shift+t"
      },
      {
        "command": "claudeAiDev.refactor",
        "key": "ctrl+alt+r",
        "mac": "cmd+alt+r"
      },
      {
        "command": "claudeAiDev.explainCode",
        "key": "ctrl+shift+e",
        "mac": "cmd+shift+e"
      },
      {
        "command": "claudeAiDev.generateCode",
        "key": "ctrl+shift+g",
        "mac": "cmd+shift+g"
      },
      {
        "command": "claudeAiDev.reviewWithAnnotations",
        "key": "ctrl+alt+shift+r",
        "mac": "cmd+alt+shift+r"
      },
      {
        "command": "claudeAiDev.browseAgents",
        "key": "ctrl+shift+b",
        "mac": "cmd+shift+b",
        "when": "!terminalFocus"
      },
      {
        "command": "claudeAiDev.generateFeature",
        "key": "ctrl+shift+f",
        "mac": "cmd+shift+f",
        "when": "!terminalFocus && !searchViewletFocus"
      },
      {
        "command": "claudeAiDev.executeTool",
        "key": "ctrl+alt+t",
        "mac": "cmd+alt+t"
      },
      {
        "command": "claudeAiDev.viewLearningInsights",
        "key": "ctrl+alt+l",
        "mac": "cmd+alt+l"
      },
      {
        "command": "claudeAiDev.manageCustomRules",
        "key": "ctrl+alt+u",
        "mac": "cmd+alt+u"
      },
      {
        "command": "claudeAiDev.viewComplianceReport",
        "key": "ctrl+alt+c",
        "mac": "cmd+alt+c"
      }
    ],
    "configuration": {
      "title": "Claude AI Development Assistant",
      "properties": {
        "claudeAiDev.backendUrl": {
          "type": "string",
          "default": "http://localhost:5050",
          "description": "Backend service URL"
        },
        "claudeAiDev.autoRoute": {
          "type": "boolean",
          "default": true,
          "description": "Automatically route queries to optimal AI"
        },
        "claudeAiDev.showMetrics": {
          "type": "boolean",
          "default": true,
          "description": "Show routing and cost metrics in responses"
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "package": "vsce package",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
